model User {
  id           Int       @id
  firstName    String
  languageCode Languages
  role         Roles
  isBot        Boolean
  isPremium    Boolean

  lastName String?
  username String?
}

model LastUserMessage {
  messageId Int @id @unique
  chatId    Int @unique
}

enum Languages {
  ru
}

enum Roles {
  admin
  user
}
